						if the result contains itemPath then
							delay 1
						else
							set isDone to true
						end if
					on error
						set isDone to true
					end try
				end repeat
				if isDone is true then
					do shell script (TerminalNotifier & " -title é®¦æ­¥é¯‡æˆé°³å”·ğŸ’‹")
					do shell script ("open " & quoted form of POSIX path of SyncDest)
					continue quit
				end if
			end if
			if RealTimeSync is "é±“å‘A->Bé®¦æ­¥" then
				do shell script (TerminalNotifier & " -title é®¦æ­¥ä¸­å”·ğŸ’‹")
				(do shell script Â¬
