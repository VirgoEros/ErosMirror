---------------------------------∞ 鱫鰰鏡之鰣空 ∞----------------------------------------ON. 
global ErosTSMirror, YouTube_download_Conversion, YouTubeDownloader_MP4toMp3, createRamDisk, ISO_APFS_OpenScript_Option, macOSinstall_USB_ISO, APFS_macOSDownload_Option, APFShelper, macOS_Download_Helper, OpenScript, rsyncOption, ErosRTsync, iCloudDriveSync, errmsg, errnbr

global APFShelper, APPname, APPpath, errmsg, errnbr
global iCloudDriveSync, APPname, APPpath, errmsg, errnbr
global createRamDisk, APPname, APPpath, errmsg, errnbr
global macOS_Download_Helper, APPname, APPpath, errmsg, errnbr
global macOSinstall_USB_ISO, APPname, APPpath, errmsg, errnbr
global ErosRTsync, APPname, APPpath, errmsg, errnbr
global YouTubeDownloader_MP4toMp3, APPname, APPpath, errmsg, errnbr
global OpenScript, OpenAppScript, TerminalNotifier, errmsg, errnbr

on run
	try
		tell application "Finder"
			set {button returned:ErosTSMirror} to ¬
				display dialog ("請嚫鱫的沝女鰰選擇 鱫鰰裸軟 選項唷💋") buttons ¬
					{"macOS實鰣鮦步 & iCloudDrive使用者鮦步", "YouTube下載 & 影音轉換MP3 & RamDisk", "macOS開機鰈鱮ISO鯯作 & APFS助手 & macOS下載助手 & 開啟鱫鰰鏡之鰣空腳本"} with title ¬
					{"鱫鰰鏡之鰣空"}
			if ErosTSMirror is "YouTube下載 & 影音轉換MP3 & RamDisk" then
				set {button returned:YouTube_download_Conversion} to ¬
					display dialog ("請嚫鱫的沝女鰰選擇 鱫鰰裸軟 選項唷💋") buttons ¬
						{"YouTube下載 & 影音轉換MP3", "RamDisk"} with title ¬
						{"鱫鰰鏡之鰣空"}
				if YouTube_download_Conversion is "YouTube下載 & 影音轉換MP3" then run script YouTubeDownloader_MP4toMp3
				if YouTube_download_Conversion is "RamDisk" then run script createRamDisk
			end if
			if ErosTSMirror is "macOS開機鰈鱮ISO鯯作 & APFS助手 & macOS下載助手 & 開啟鱫鰰鏡之鰣空腳本" then
				set {button returned:ISO_APFS_OpenScript_Option} to ¬
					display dialog ("請嚫鱫的沝女鰰選擇 鱫鰰裸軟 選項唷💋") buttons ¬
						{"macOS開機鰈鱮ISO鯯作", "APFS助手 & macOS下載助手", "開啟鱫鰰鏡之鰣空腳本"} with title ¬
						{"鱫鰰鏡之鰣空"}
				if ISO_APFS_OpenScript_Option is "macOS開機鰈鱮ISO鯯作" then run script macOSinstall_USB_ISO
				if ISO_APFS_OpenScript_Option is "APFS助手 & macOS下載助手" then
					set {button returned:APFS_macOSDownload_Option} to ¬
						display dialog ("請嚫鱫的沝女鰰選擇 鱫鰰裸軟 選項唷💋") buttons ¬
							{"APFS助手", "macOS下載助手"} with title ¬
							{"鱫鰰鏡之鰣空"}
					if APFS_macOSDownload_Option is "APFS助手" then run script APFShelper
					if APFS_macOSDownload_Option is "macOS下載助手" then run script macOS_Download_Helper
				end if
				if ISO_APFS_OpenScript_Option is "開啟鱫鰰鏡之鰣空腳本" then run script OpenScript
			end if
			if ErosTSMirror is "macOS實鰣鮦步 & iCloudDrive使用者鮦步" then
				set {button returned:rsyncOption} to ¬
					display dialog ("請嚫鱫的沝女鰰選擇 macOS實鰣鮦步 選項唷💋") buttons ¬
						{"macOS實鰣鮦步", "iCloudDrive使用者鮦步"} with title ¬
						{"鱫鰰鏡之鰣空"}
				if rsyncOption is "macOS實鰣鮦步" then run script ErosRTsync
				if rsyncOption is "iCloudDrive使用者鮦步" then run script iCloudDriveSync
			end if
		end tell
	on error errmsg number errnbr
		if errnbr = -128 then continue quit
		continue quit
	end try
end run

script APFShelper
	try
		set APPname to ("鱫鰰APFS助手")
		set APPpath to POSIX path of (path to me as text) ¬
			& {"Contents/Resources/Scripts/" & APPname & ".app"}
		set APPpath to quoted form of APPpath
		(do shell script (" open -n " & APPpath))
		continue quit
	on error errmsg number errnbr
		if errnbr = -128 then continue quit
		continue quit
	end try
end script

script iCloudDriveSync
	try
		set APPname to ("鱫鰰iCloudDrive使用者鮦步")
		set APPpath to POSIX path of (path to me as text) ¬
			& {"Contents/Resources/Scripts/" & APPname & ".app"}
		set APPpath to quoted form of APPpath
		(do shell script (" open -n " & APPpath))
		continue quit
	on error errmsg number errnbr
		if errnbr = -128 then continue quit
		continue quit
	end try
end script

